{
    "$schema": "https://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "title": "Root Schema",
    "type": "object",
    "default": {},
    "required": [
        "title",
        "description",
        "instructions",
        "tag"
    ],
    "properties": {
        "title": {
            "title": "The title Schema",
            "type": "string",
            "default": "",
            "examples": [
                "This is the title"
            ]
        },
        "description": {
            "title": "The description Schema",
            "type": "string",
            "default": "",
            "examples": [
                "This is the description"
            ]
        },
        "instructions": {
            "title": "The instructions Schema",
            "type": "string",
            "default": "",
            "examples": [
                "This is the instructions section."
            ]
        },
        "tag": {
            "title": "The tag Schema",
            "type": "string",
            "default": "",
            "examples": [
                "Tag"
            ]
        }
    },
    "examples": [{
        "title": "This is the title",
        "description": "This is the description",
        "instructions": "This is the instructions section.",
        "tag": "Tag"
    }]
  }